<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="ap" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.demo3.FileChooserController">
    <children>
        <ImageView fx:id="imageView" fitHeight="224.0" fitWidth="396.0" layoutX="100.0" layoutY="50.0"
                   pickOnBounds="true" preserveRatio="true"/>
        <Button layoutX="256.0" layoutY="300.0" mnemonicParsing="false" onAction="#chooseImage" text="Open"/>
    </children>
</AnchorPane>